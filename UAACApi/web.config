<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <modules>
        <remove name="WebDAVModule"/>
      </modules>

      <handlers>
        <!-- Remove WebDAV module so that we can make DELETE or PUT requests -->
        <remove name="WebDAV" />
      </handlers>
    </system.webServer>
  </location>
</configuration>